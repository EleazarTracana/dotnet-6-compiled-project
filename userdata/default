[Unit]
Description=Start the Website

[Service]
WorkingDirectory=/var/www/app
ExecStart=/usr/bin/dotnet/dotnet /var/www/app/app.dll
Restart=always
RestartSec=10
SyslogIdentifier=website
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false

[Install]
WantedBy=multi-user.target
ubuntu@ip-172-31-88-0:/etc/systemd/system$ cd ../
ubuntu@ip-172-31-88-0:/etc/systemd$ cd /etc/nginx/sites-available
ubuntu@ip-172-31-88-0:/etc/nginx/sites-available$ ;s
bash: syntax error near unexpected token `;'
ubuntu@ip-172-31-88-0:/etc/nginx/sites-available$ ls
default
ubuntu@ip-172-31-88-0:/etc/nginx/sites-available$ cat default
    listen        80;
    location / {
        proxy_pass         http://127.0.0.1:5000;
 proxy_http_version 1.1;
  proxy_set_header   Upgrade ;
     proxy_set_header   Connection keep-alive;
        proxy_set_header   Host ;
        proxy_cache_bypass ;
        proxy_set_header   X-Forwarded-For ;
        proxy_set_header   X-Forwarded-Proto ;
    }
}